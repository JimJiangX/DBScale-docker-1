{
  "metric_class": "host_swarm_agent",
  "interval_time": 60,
  "exec_file": "host_swarm_agent.sh",
  "params": "",
  "enable": true,
  "metric_type": "host",
  "metrics": [
    {
      "metric": "host.swarm_agent_status",
      "event_levels": [
        {
          "level": "critical",
          "value": "critical",
          "event_level_enable": true,
          "alarm_enable": true
        },
        {
          "level": "warning",
          "value": "",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "passing",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": false,
      "event": true,
      "event_type": "string_type",
      "value_type": "string",
      "counter": 1440
    }
  ]
}
