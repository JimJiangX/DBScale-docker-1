{
  "metric_class": "host_fs",
  "interval_time": 60,
  "exec_file": "host_fs.sh",
  "params": "/home /",
  "enable": true,
  "metric_type": "host",
  "metrics": [
    {
      "metric": "host.homedir_total",
      "event_levels": [
        {
          "level": "critical",
          "value": "80-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "50-79",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-49",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": false,
      "event": false,
      "event_type": "data_type",
      "value_type": "int64",
      "counter": 1440
    },
    {
      "metric": "host.homedir_usage",
      "event_levels": [
        {
          "level": "critical",
          "value": "80-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "50-89",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-49",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    },
    {
      "metric": "host.rootdir_total",
      "event_levels": [
        {
          "level": "critical",
          "value": "90-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "50-89",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-49",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": false,
      "event": false,
      "event_type": "data_type",
      "value_type": "int64",
      "counter": 1440
    },
    {
      "metric": "host.rootdir_usage",
      "event_levels": [
        {
          "level": "critical",
          "value": "80-100",
          "event_level_enable": true,
          "alarm_enable": true
        },
        {
          "level": "warning",
          "value": "50-79",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-49",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    }
  ]
}
