{
  "metric_class": "unit_mysql_buffer",
  "interval_time": 60,
  "exec_file": "unit_mysql_buffer.sh",
  "params": "",
  "enable": true,
  "metric_type": "unit_mysql",
  "metrics": [
    {
      "metric": "unit_mysql.buffer_pool_hit",
      "event_levels": [
        {
          "level": "critical",
          "value": "0-79",
          "event_level_enable": true,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "80-89",
          "event_level_enable": true,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "90-95",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "96-100",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    },
