{
  "metric_class": "unit_mysql_lock",
  "interval_time": 60,
  "exec_file": "unit_mysql_lock.sh",
  "params": "",
  "enable": true,
  "metric_type": "unit_mysql",
  "metrics": [
    {
      "metric": "unit_mysql.lock_wait_info",
      "event_levels": [
        {
          "level": "critical",
          "value": "90-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "70-89",
          "event_level_enable": true,
          "alarm_enable": true
        },
        {
          "level": "normal",
          "value": "40-69",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": false,
      "event_type": "string_type",
      "value_type": "string",
      "counter": 1440
    }
  ]
}
