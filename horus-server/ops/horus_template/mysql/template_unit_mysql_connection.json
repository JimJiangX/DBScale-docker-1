{
  "metric_class": "unit_mysql_connection",
  "interval_time": 60,
  "exec_file": "unit_mysql_connection.sh",
  "params": "",
  "enable": true,
  "metric_type": "unit_mysql",
  "metrics": [
    {
      "metric": "unit_mysql.connection_max",
      "event_levels": [
        {
          "level": "critical",
          "value": "90-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "70-89",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-69",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": false,
      "event_type": "data_type",
      "value_type": "int64",
      "counter": 1440
    },
    {
      "metric": "unit_mysql.connection_attempts",
