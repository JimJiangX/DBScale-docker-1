{
  "metric_class": "container_cpu",
  "interval_time": 60,
  "exec_file": "container_cpu.sh",
  "params": "",
  "enable": true,
  "metric_type": "container",
  "metrics": [
    {
      "metric": "container.all_cpu_usage",
      "event_levels": [
        {
          "level": "critical",
          "value": "90-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "70-89",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-69",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    },
    {
      "metric": "container.avg_cpu_usage",
      "event_levels": [
        {
          "level": "critical",
          "value": "80-100",
          "event_level_enable": true,
          "alarm_enable": true
        },
        {
          "level": "warning",
          "value": "60-79",
          "event_level_enable": true,
          "alarm_enable": false 
        },
        {
          "level": "normal",
          "value": "40-59",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    }
  ]
}
