{
  "metric_class": "host_cpu",
  "interval_time": 60,
  "exec_file": "host_cpu.sh",
  "params": "",
  "enable": true,
  "metric_type": "host",
  "metrics": [
    {
      "metric": "host.cpu_usage",
      "event_levels": [
        {
          "level": "critical",
          "value": "90-100",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "warning",
          "value": "50-89",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "normal",
          "value": "40-49",
          "event_level_enable": false,
          "alarm_enable": false
        },
        {
          "level": "info",
          "value": "0-39",
          "event_level_enable": false,
          "alarm_enable": false
        }
      ],
      "history": true,
      "event": true,
      "event_type": "data_type",
      "value_type": "float64",
      "counter": 1440
    }
  ]
}
